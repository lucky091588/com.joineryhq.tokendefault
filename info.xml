<?xml version="1.0"?>
<extension key="com.joineryhq.tokendefault" type="module">
  <file>tokendefault</file>
  <name>Token Default</name>
  <description>This extension can replace the token values in the mailing with your own custom values.</description>
  <license>GPL-3.0</license>
  <maintainer>
    <author>Allen Shaw</author>
    <email>allen@JoineryHQ.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/twomice/com.joineryhq.tokendefault</url>
    <url desc="Documentation">https://github.com/twomice/com.joineryhq.tokendefault</url>
    <url desc="Support">https://github.com/twomice/com.joineryhq.tokendefault/issue</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2020-09-15</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Tokendefault</namespace>
  </civix>
</extension>
